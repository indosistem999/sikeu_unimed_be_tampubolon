{
  "/user-log-activity": {
    "get": {
      "tags": ["User Log Activity"],
      "security": [
        {
          "bearerAuth": []
        }
      ],
      "parameters": [
        {
          "in": "query",
          "name": "page",
          "required": false,
          "schema": {
            "type": "number"
          },
          "description": "Page value is number. ex : ?page=1"
        },
        {
          "in": "query",
          "name": "limit",
          "required": false,
          "schema": {
            "type": "integer"
          },
          "description": "Limit value is number by default limit is 10. ex : ?limit=100"
        },
        {
          "in": "query",
          "name": "direction_name",
          "required": false,
          "schema": {
            "type": "array or string"
          },
          "description": "Example --> direction_name=module_name, direction_name=order_number"
        },
        {
          "in": "query",
          "name": "order_name",
          "required": false,
          "schema": {
            "type": "array or string"
          },
          "description": "Example --> order_name=asc, order_name=desc"
        },
        {
          "in": "query",
          "name": "search",
          "required": false,
          "schema": {
            "type": "string"
          },
          "description": "Example --> search=Pengaturan"
        }
      ],
      "responses": {
        "200": {
          "description": "OK",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DefaultResponsePaginationSchema"
              }
            }
          }
        }
      }
    }
  }
}
